<mvc:View controllerName="regesta.regestarapportini.controller.NavbarFooter"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m"
	xmlns:f="sap.f">


	<Page>
		<footer class="footer">
			<Toolbar>
				<ToolbarSpacer/>
				<Button icon="sap-icon://activities" press="goToTickets" />
				<ToolbarSpacer/>
				<ToolbarSpacer/>
				<Button icon="sap-icon://fob-watch" press="checkHours" id="hourBadge">
				</Button>
				<ToolbarSpacer/>
				<ToolbarSpacer/>
				<Button icon="sap-icon://add" press="showPopup"/>
				<ToolbarSpacer/>
				<ToolbarSpacer/>
				<Button icon="sap-icon://receipt" press="goToRapportini"/>
				<ToolbarSpacer/>
			</Toolbar>
		</footer>
	</Page>


</mvc:View>